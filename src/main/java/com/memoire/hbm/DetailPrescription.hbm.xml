<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="DetailPrescription" table="detailprescription">
  <id name="idDetail" type="int">
   <column name="iddetail"/>
   <generator class="assigned"/>
  </id>
  <many-to-one class="Prescription" fetch="select" name="prescription">
   <column name="idprescription" not-null="true"/>
  </many-to-one>
  <property generated="never" lazy="false" name="libelle" type="string">
   <column length="254" name="libelle"/>
  </property>
  <property generated="never" lazy="false" name="produit" type="string">
   <column length="254" name="produit"/>
  </property>
  <property generated="never" lazy="false" name="quantite" type="int">
   <column name="quantite"/>
  </property>
 </class>
</hibernate-mapping>
