<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="Nature" table="nature">
  <id name="idNature" type="int">
   <column name="idnature"/>
   <generator class="assigned"/>
  </id>
  <property generated="never" lazy="false" name="libelle" type="string">
   <column length="254" name="libelle"/>
  </property>
  <property generated="never" lazy="false" name="nombre" type="int">
   <column name="nombre"/>
  </property>
 </class>
</hibernate-mapping>
