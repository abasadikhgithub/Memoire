<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="ResultatAnalyse" table="resultatanalyse">
  <id name="idResultat" type="int">
   <column name="idresultat"/>
   <generator class="assigned"/>
  </id>
  <many-to-one class="Fichier" fetch="select" name="fichier">
   <column name="idfichier" not-null="true"/>
  </many-to-one>
  <many-to-one class="Analyse" fetch="select" name="analyse">
   <column name="idacte" not-null="true"/>
  </many-to-one>
  <property generated="never" lazy="false" name="interpretation" type="string">
   <column length="254" name="interpretation"/>
  </property>
 </class>
</hibernate-mapping>
