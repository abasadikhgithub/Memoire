<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="RendezVous" table="rendezvous">
  <id name="idRendezVous" type="int">
   <column name="idrendezvous"/>
   <generator class="assigned"/>
  </id>
  <many-to-one class="Medecin" fetch="select" name="medecin">
   <column name="idutilisateur" not-null="true"/>
  </many-to-one>
  <many-to-one class="Patient" fetch="select" name="patient">
   <column name="pat_idutilisateur" not-null="true"/>
  </many-to-one>
  <property generated="never" lazy="false" name="dateHeure" type="date">
   <column name="dateheure"/>
  </property>
  <property generated="never" lazy="false" name="statut" type="string">
   <column length="254" name="statut"/>
  </property>
  <property generated="never" lazy="false" name="motif" type="string">
   <column length="254" name="motif"/>
  </property>
 </class>
</hibernate-mapping>
