<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Facture" table="facture">
        <id name="idFacture" type="int">
            <column name="idfacture" />
            <generator class="assigned" />
        </id>
        <property name="montant" type="int">
            <column name="montant" />
        </property>
        <property name="statut" type="string">
            <column name="statut" length="254" />
        </property>
        <property name="dateEmission" type="date">
            <column name="dateemission" />
        </property>
    </class>
</hibernate-mapping>