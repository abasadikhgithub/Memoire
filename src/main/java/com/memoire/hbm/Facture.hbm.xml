<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="Facture" table="facture">
  <id name="idFacture" type="int">
   <column name="idfacture"/>
   <generator class="assigned"/>
  </id>
  <property generated="never" lazy="false" name="montant" type="int">
   <column name="montant"/>
  </property>
  <property generated="never" lazy="false" name="statut" type="string">
   <column length="254" name="statut"/>
  </property>
  <property generated="never" lazy="false" name="dateEmission" type="date">
   <column name="dateemission"/>
  </property>
 </class>
</hibernate-mapping>
