<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Type" table="type">
        <id name="idType" type="int">
            <column name="idtype" />
            <generator class="assigned" />
        </id>
        <many-to-one name="structureSanitaire" class="StructureSanitaire" fetch="select">
            <column name="idstructure" />
        </many-to-one>
        <property name="estPublic" type="boolean">
            <column name="estpublic" />
        </property>
        <property name="nbrePrive" type="int">
            <column name="nbreprive" />
        </property>
        <property name="nbrePublic" type="int">
            <column name="nbrepublic" />
        </property>
    </class>
</hibernate-mapping>