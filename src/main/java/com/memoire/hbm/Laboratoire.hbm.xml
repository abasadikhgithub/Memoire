<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="Laboratoire" table="laboratoire">
  <id name="idLaboratoire" type="int">
   <column name="idlaboratoire"/>
   <generator class="assigned"/>
  </id>
  <many-to-one class="StructureSanitaire" fetch="select" name="structureSanitaire">
   <column name="idstructure"/>
  </many-to-one>
  <property generated="never" lazy="false" name="fixe" type="int">
   <column name="fixe"/>
  </property>
  <property generated="never" lazy="false" name="localisationGPS" type="string">
   <column length="254" name="localisationgps"/>
  </property>
  <property generated="never" lazy="false" name="siteWeb" type="string">
   <column length="254" name="siteweb"/>
  </property>
 </class>
</hibernate-mapping>
