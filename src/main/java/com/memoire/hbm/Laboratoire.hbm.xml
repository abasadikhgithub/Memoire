<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Laboratoire" table="laboratoire">
        <id name="idLaboratoire" type="int">
            <column name="idlaboratoire" />
            <generator class="assigned" />
        </id>
        <many-to-one name="structureSanitaire" class="structuresanitaire" fetch="select">
            <column name="idstructure" />
        </many-to-one>
        <property name="fixe" type="int">
            <column name="fixe" />
        </property>
        <property name="localisationGPS" type="string">
            <column name="localisationgps" length="254" />
        </property>
        <property name="siteWeb" type="string">
            <column name="siteweb" length="254" />
        </property>
    </class>
</hibernate-mapping>