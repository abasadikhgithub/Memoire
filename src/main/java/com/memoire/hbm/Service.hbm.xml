<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="Service" table="service">
  <id name="idService" type="int">
   <column name="idservice"/>
   <generator class="assigned"/>
  </id>
  <property generated="never" lazy="false" name="nom" type="string">
   <column length="254" name="nom"/>
  </property>
  <property generated="never" lazy="false" name="description" type="string">
   <column length="254" name="description"/>
  </property>
  <property generated="never" lazy="false" name="disponible" type="boolean">
   <column name="disponible"/>
  </property>
 </class>
</hibernate-mapping>
