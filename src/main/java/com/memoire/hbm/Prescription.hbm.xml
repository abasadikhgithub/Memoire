<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="Prescription" table="prescription">
  <id name="idPrescription" type="int">
   <column name="idprescription"/>
   <generator class="assigned"/>
  </id>
  <many-to-one class="Consultation" fetch="select" name="consultation">
   <column name="idacte" not-null="true"/>
  </many-to-one>
  <property generated="never" lazy="false" name="type" type="string">
   <column length="254" name="type"/>
  </property>
  <property generated="never" lazy="false" name="analyse" type="string">
   <column length="254" name="analyse"/>
  </property>
 </class>
</hibernate-mapping>
