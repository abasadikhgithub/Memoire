<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="NecessiteDetail" table="necessitedetail">
        <composite-id>
            <key-property name="idDetail" column="iddetail" type="int"/>
            <key-property name="idNecessite" column="idnecessite" type="int"/>
        </composite-id>
    </class>
</hibernate-mapping>
