<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="Notification" table="notification">
  <id name="idNotification" type="int">
   <column name="idnotification"/>
   <generator class="assigned"/>
  </id>
  <many-to-one class="Utilisateur" fetch="select" name="utilisateur">
   <column name="idutilisateur" not-null="true"/>
  </many-to-one>
  <property generated="never" lazy="false" name="type" type="string">
   <column length="254" name="type"/>
  </property>
  <property generated="never" lazy="false" name="contenu" type="string">
   <column length="254" name="contenu"/>
  </property>
  <property generated="never" lazy="false" name="dateEnvoi" type="string">
   <column length="254" name="dateenvoi"/>
  </property>
  <property generated="never" lazy="false" name="estVisible" type="boolean">
   <column name="estvisible"/>
  </property>
 </class>
</hibernate-mapping>
