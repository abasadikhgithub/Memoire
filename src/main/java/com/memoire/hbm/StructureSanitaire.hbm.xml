<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="StructureSanitaire" table="structuresanitaire">
  <id name="idStructure" type="int">
   <column name="idstructure"/>
   <generator class="assigned"/>
  </id>
  <many-to-one class="Nature" fetch="select" name="nature">
   <column name="idnature" not-null="true"/>
  </many-to-one>
  <many-to-one class="AgentEtat" fetch="select" name="agentEtat">
   <column name="idutilisateur" not-null="true"/>
  </many-to-one>
  <property generated="never" lazy="false" name="nom" type="string">
   <column length="254" name="nom"/>
  </property>
  <property generated="never" lazy="false" name="adresse" type="string">
   <column length="254" name="adresse"/>
  </property>
  <property generated="never" lazy="false" name="telephone" type="long">
   <column name="telephone"/>
  </property>
  <property generated="never" lazy="false" name="email" type="string">
   <column length="254" name="email"/>
  </property>
 </class>
</hibernate-mapping>
