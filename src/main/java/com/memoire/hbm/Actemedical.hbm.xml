<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
    <class name="ActeMedical" table="actemedical">
        <id name="idActe" type="int">
            <column name="idacte" />
            <generator class="assigned" />
        </id>
        <many-to-one name="facture" class="Facture" fetch="select">
            <column name="idfacture" not-null="true" />
        </many-to-one>
        <many-to-one name="patient" class="Patient" fetch="select">
            <column name="idutilisateur_patient" not-null="true" />
        </many-to-one>
        <property name="date" type="date">
            <column name="date" />
        </property>
        <property name="type" type="string">
            <column name="type" length="254" />
        </property>
        <union-subclass name="Analyse" table="&quot;Analyse&quot;">
            <property name="numero" column="numero" />
            <property name="nom" column="nom" />
            <property name="description" column="description" />
            <property name="datePrescription" column="dateprescription" />
            <property name="dateRealisation" column="daterealisation" />
            <property name="urgence" column="urgence" />
            <many-to-one name="laboratoire" class="Laboratoire">
                <column name="idlaboratoire" not-null="true" />
            </many-to-one>
            <many-to-one name="medecin" class="Medecin">
                <column name="idutilisateur_medecin" not-null="true" />
            </many-to-one>
            <many-to-one name="patientAnalyse" class="Patient">
                <column name="idutilisateur_patientanalyse" not-null="true" />
            </many-to-one>
        </union-subclass>
        <union-subclass name="Consultation" table="consultation">
            <property name="dateHeure" column="dateheure" />
            <property name="notesMedecin" column="notesmedecin" />
            <many-to-one name="sessionVideoconference" class="SessionVideoconference">
                <column name="idsession" />
            </many-to-one>
            <many-to-one name="rendezVous" class="RendezVous">
                <column name="idrendezvous" />
            </many-to-one>
            <many-to-one name="medecinConsultation" class="Medecin">
                <column name="idutilisateur_medecin_consultation" />
            </many-to-one>
        </union-subclass>
    </class>
</hibernate-mapping>