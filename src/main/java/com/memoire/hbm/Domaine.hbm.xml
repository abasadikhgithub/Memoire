<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="Domaine" table="domaine">
  <id name="idDomaine" type="int">
   <column name="iddomaine"/>
   <generator class="assigned"/>
  </id>
  <property generated="never" lazy="false" name="famille" type="string">
   <column length="254" name="famille"/>
  </property>
 </class>
</hibernate-mapping>
