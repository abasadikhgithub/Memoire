<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.memoire.model">
 <class name="DonneeCapteur" table="donneecapteur">
  <id name="id" type="int">
   <column name="id"/>
   <generator class="assigned"/>
  </id>
  <many-to-one class="Capteur" fetch="select" name="capteur">
   <column name="idcapteur" not-null="true"/>
  </many-to-one>
  <property generated="never" lazy="false" name="valeur" type="big_decimal">
   <column name="valeur" precision="10" scale="2"/>
  </property>
  <property generated="never" lazy="false" name="dateReception" type="date">
   <column name="datereception"/>
  </property>
 </class>
</hibernate-mapping>
