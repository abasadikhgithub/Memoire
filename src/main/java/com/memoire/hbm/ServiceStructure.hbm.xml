<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ServiceStructure" table="servicestructure">
        <composite-id>
            <key-property name="idStructure" column="idstructure" type="int"/>
            <key-property name="idService" column="idservice" type="int"/>
        </composite-id>
    </class>
</hibernate-mapping>